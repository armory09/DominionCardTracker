@model DominionCardTracker.Models.Tables.CardSet
@{
    ViewBag.Title = "Create";
}
<h2>Create</h2>
<div class="row">
    @using (Html.BeginForm("Create", "CardSet", FormMethod.Post, new { @class = "col-md-4" }))
    {
        <fieldset>
            <legend>Create Card Set</legend>
            <div class="form-group">
                <label>Card Set Name</label><br />
                @Html.TextBoxFor(m => m.CardSetName, new { placeholder = "enter set name" })
            </div>
            <div class="form-inline">
                <button type="submit" class="btn btn-primary">Save Changes</button>
                @Html.ActionLink("Cancel", "Index", "CardSet", null, new { @class = "btn" })
            </div>
        </fieldset>
    }
</div>
